@model Charcoal.Web.Models.ProjectViewModel
<div class="row" id="projectdetail">
    <div class="span8" >
        <div id="projectname" class="span4"> @Model.Name</div>
        <div id="velocity" class="span4"> @Model.Velocity</div>
    </div>
    <div class="span20">
        <div class="row">
            <div class="span4">
                @Html.ActionLink("Current", "CurrentIteration", "Stories", new { projectId = @Model.Id }, new { @class = "btn success" })
            </div>
            <div class="span4">
                @Html.ActionLink("Backlog", "BackLogStories", "Stories", new { projectId = @Model.Id }, new { @class = "btn" })
            </div>
            <div class="span4">
                @Html.ActionLink("Icebox", "IceboxStories", "Stories", new { projectId = @Model.Id }, new { @class = "btn info" })
            </div>
            <div class="span4">
                @Html.ActionLink("Analyze", "AnalyzeProject", "Analytics", new { projectId = @Model.Id, velocity= @Model.Velocity, name=@Model.Name}, new { @class = "btn info" })
            </div>
            <div class="span4">
                @Html.ActionLink("Analyze Tag", "AnalyzeTag", "Analytics", new { projectId = @Model.Id}, new { @class = "btn info" })
            </div>
        </div>
    </div>
    <span class="label">@Model.Description</span>
</div>